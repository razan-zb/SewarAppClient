{
  "expo": {
    "name": "SewarAppClient",
    "slug": "SewarAppClient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SewarLogo1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.razan18.SewarAppClient"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SewarLogo1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.razan18.SewarAppClient"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SewarLogo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SewarLogo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "371e9bec-61ac-46ba-8508-8d2bdcb45db5"
      }
    }
  }
}
